{
  "name": "es6-redux",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:dev": "browserify src/main.js --extension=.jsx -t [ babelify --sourceMapsAbsolute ] --outfile dist/bundle.js",
    "start": "npm run build:dev && npm-run-all --parallel watch browser",
    "watch": "onchange src -- npm run build:dev",
    "lint-fix": "eslint --fix src/",
    "browser": "live-server --entry-file='index.html' --watch=dist/ --wait=100  --no-browser",
    "browser80": "live-server --port=80 --no-browser --entry-file='index.html' --watch=dist/ --wait=100"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "live-server": "^1.0.0",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "firebase": "^3.0.5",
    "material-ui": "^0.15.1",
    "node-uuid": "^1.4.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
