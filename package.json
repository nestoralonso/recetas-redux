{
  "name": "es6-redux",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:dev": "browserify src/main.js -t babelify --outfile dist/bundle.js",
    "browse": "browser-sync start --s --index 'index.html' --files 'html/**/*.html, dist/**/*.js' --no-notify",
    "start": "npm run build:dev && npm-run-all --parallel watch",
    "test": "npm run build:dev",
    "watch": "onchange src -- npm run build:dev",
    "browser": "live-server --entry-file='index.html'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.12",
    "browserify": "^13.0.1",
    "live-server": "^1.0.0",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1"
  }
}
